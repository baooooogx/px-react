{
  "name": "px-react",
  "version": "1.0.0",
  "description": "react webpack for px-coupon project",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist *.zip",
    "start": "NODE_ENV=development npm run clean && webpack --config ./build/webpack.dev.js",
    "build": "npm run clean && webpack --config ./build/webpack.release.js -p",
    "server": "node app.js",
    "watch": "NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js --hot --inline --progress --colors --content-base ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baooooogx/px-react.git"
  },
  "keywords": [
    "px-coupon",
    "react",
    "webpack",
    "es6"
  ],
  "author": "baoguanxia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/baooooogx/px-react.git"
  },
  "homepage": "https://github.com/baooooogx/px-react.git",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "connected-react-router": "^2.0.0-alpha.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jquery": "^2.2.3",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "path": "^0.12.7",
    "postcss-loader": "^1.2.1",
    "react-hot-loader": "^1.3.0",
    "react-transform-hmr": "^1.0.2",
    "resolve-url-loader": "^1.6.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.15",
    "webpack-cleanup-plugin": "^0.4.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.15.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "redux": "^3.6.0"
  }
}
